<div class="col-sm-12">
    <h2>Get your answers now!</h2>
    <div class="input-group">
        <input type="text" class="form-control" placeholder="Type in your question...">
        <span class="input-group-btn">
          <button class="btn btn-success" type="button">Ask a <i>bunch</i> of Advisors</button>
        </span>
    </div>
    <br/>
    <br/>
    <br/>
    <h3>Responses from our advisors...</h3>
    <br/>
    <ul class="media-list advisor-message-list">
        {{#each message in messageFromAdvisors}}
          {{user-message profileImageUrl=message.profileImageUrl advisorName=message.advisorName postedDate=message.postedDate messageText=message.messageText starRating=message.starRating}}
          <br/>
        {{/each}}
    </ul>
</div>


